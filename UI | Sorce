-- Player stuff

PT:AddSlider({ -- This Slider changes your Walkspeed to change how fast u are
	Name = "Speed",
	Min = 16,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(0,255,0),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

PT:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 1500,
	Default = 16,
	Color = Color3.fromRGB(0,255,0),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})

