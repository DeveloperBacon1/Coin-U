-- Autos
local Animate = game.Players.LocalPlayer.Character.Animate -- Checks Your charater for a local script called Animate to change your animation pacl
game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true -- Turns on Jump Power and turns off Jump Height

-- Main

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "CoinU", HidePremium = false, SaveConfig = true, IntroText = "CoinU", ConfigFolder = "CoinU"})
-- Tabs
local HT = Window:MakeTab({
	Name = "Home",
	PremiumOnly = false
})


